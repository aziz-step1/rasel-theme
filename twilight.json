      "fields": [
        {
          "type": "repeater",
          "id": "slides",
          "label": "شرائح السلايدر",
          "min": 1,
          "max": 5,
          "fields": [
            {
              "type": "image",
              "id": "image",
              "label": "الصورة"
            },
            {
              "type": "string",
              "id": "title",
              "label": "العنوان",
              "multilanguage": true
            },
            {
              "type": "string",
              "id": "subtitle",
              "label": "نص فرعي",
              "multilanguage": true
            },
            {
              "type": "string",
              "id": "button_text",
              "label": "نص الزر",
              "multilanguage": true
            },
            {
              "type": "url",
              "id": "button_link",
              "label": "رابط الزر"
            }
          ]
        }
      ]
    },
    {
      "key": "banner",
      "title": "بانر عرض",
      "icon": "sicon-banner",
      "image": "https://cdn.salla.sa/images/components/wide-banner.png",
      "path": "home.banner",
      "fields": []
    },
    {
      "key": "categories",
      "title": "تصنيفات دائرية",
      "icon": "sicon-category",
      "image": "https://cdn.salla.sa/images/components/round-categories.png",
      "path": "home.categories",
      "fields": []
