        },
        {
          "type": "string",
          "id": "sub_title",
          "label": "وصف القسم",
          "multilanguage": true
        },
        {
          "type": "items",
          "id": "products",
          "label": "المنتجات",
          "format": "dropdown-list",
          "source": "products",
          "multichoice": true
        }
      ]
    },
    {
      "key": "rasel-slider",
      "title": "سلايدر رسيل",
      "icon": "sicon-slider",
      "image": "assets/images/rasel-slider-preview.png",
